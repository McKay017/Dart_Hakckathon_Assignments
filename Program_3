void main() {
  int marks = 64;
  determineGrade(marks);
}

void determineGrade(int marks) {
  if (marks > 85) {
    print("Excellent");
  } else if (marks >= 75 && marks <= 84) {
    print("Very Good");
  } else if (marks >= 65 && marks < 74) {
    print("Good");
  } else {
    print("Average");
  }
}
